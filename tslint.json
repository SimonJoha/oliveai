{
  "defaultSeverity": "warning",
  "extends": [
    "tslint-config-airbnb"
  ],
  "linterOptions": {
    "formatter": "codeFrame",
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "eofline": false,
    "no-boolean-literal-compare": true,
    "import-name": false,
    "trailing-comma": false,
    "curly": false,
    "brace-style": false,
    "no-increment-decrement": false
  }
}
